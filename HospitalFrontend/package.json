{
  "name": "HospitalFrontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "preprocess src/api_preprocess/api.js . -SERVER_URI=localhost:5000 > src/constant/api.js && vue-cli-service serve",
    "build": "cross-var preprocess src/api_preprocess/api.js . -SERVER_URI=localhost:5000 > src/constant/api.js && vue-cli-service build",
    "dockerbuild": "cross-var preprocess src/api_preprocess/api.js . -SERVER_URI=$ASPNET_HOST:$ASPNET_PORT > src/constant/api.js && vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "core-js": "^3.6.5",
    "cross-var": "^1.1.0",
    "jquery": "^3.5.1",
    "moment": "^2.29.1",
    "preprocessor": "^1.4.0",
    "toastify-js": "^1.9.3",
    "vue": "^3.0.0",
    "vue-axios": "^3.1.3",
    "vue-js-modal": "^2.0.0-rc.6",
    "vue-router": "^4.0.0-0",
    "vue-star-rating": "^2.1.0",
    "vue3-datepicker": "^0.1.1",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.11.5",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "^4.5.8",
    "@vue/cli-plugin-vuex": "^4.5.9",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^4.5.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "mutationobserver-shim": "^0.3.7",
    "popper.js": "^1.16.1",
    "portal-vue": "^2.1.7",
    "sass": "^1.26.11",
    "sass-loader": "^10.0.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-console": "off",
      "no-unused-vars": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
