<Window x:Class="WPFHospitalEditor.Building1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFHospitalEditor"
        mc:Ignorable="d"
        Title="Building1" Height="650" Width="870" MinHeight="650" MinWidth="870" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" Background="#FFC6F5F8">
    <StackPanel Orientation="Vertical">
        <Canvas MinHeight="400" Margin="20,20,20,20" Background="LightGray" Name="canvas1" MouseLeftButtonDown="displayAdditionalInformation"/>
        <StackPanel Orientation="Horizontal">
            <Button Background="#BDE7D8" Height="30" Width="100" Name="back" Content="Nazad" Margin="20,0,20,30" HorizontalAlignment="Left" Click="back_Click"/>
        </StackPanel>
    </StackPanel>
</Window>
